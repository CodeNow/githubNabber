{
  "name": "githubNabber",
  "version": "0.0.1",
  "description": "Building Docker images from Github",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=testing NODE_PATH=./lib mocha --bail --reporter spec",
    "docker-test": "TEST_USING_DOCKER=true NODE_ENV=testing NODE_PATH=./lib mocha --bail --reporter spec",
    "coverage": "NODE_ENV=testing NODE_PATH=./lib mocha --bail --require blanket --reporter html-cov > coverage.html",
    "coverage-docker": "TEST_USING_DOCKER=true NODE_ENV=testing NODE_PATH=./lib mocha --bail --require blanket --reporter html-cov > coverage.html",
    "travis": "NODE_ENV=testing NODE_PATH=./lib mocha --bail --require blanket --reporter travis-cov",
    "test-all": "npm test && npm run docker-test",
    "coverage-all": "npm run coverage && npm run coverage-docker && npm run travis"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    },
    "travis-cov": {
      "threshold": 90
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "request": "~2.33.0",
    "npm": "~1.3.26",
    "tar-stream": "~0.2.5",
    "install": "~0.1.7",
    "dockerode": "~1.2.5",
    "express": "~3.4.8",
    "concat-stream": "~1.4.1"
  },
  "devDependencies": {
    "docker-mock": "git://github.com/Runnable/docker-mock#develop",
    "express": "~3.4.8",
    "mocha": "^1.18.2",
    "should": "~3.3.0",
    "tar": "~0.1.19",
    "blanket": "~1.1.6",
    "travis-cov": "~0.2.5"
  }
}
